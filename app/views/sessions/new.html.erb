<h1 align="center">Log in</h1>
<br>
<div align="center">
<%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :email %><br>
    <%= f.text_field :email %><br><br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br>
      <br>
    <%= f.submit "Log in"%><br>
<% end %>
</div>
<div class="btn btn-success topcorner">
  <%= link_to 'Back', root_path, :style =>'color:#FFFFFF;' %></div>