<%= form_for(@histories) do |history|%>


    <div class="field">
      <%= history.label :from %>
      <%= history.datetime_select :from, id: :reservation_from %>
    </div>

    <div class="field">
      <%= history.label :to %>
      <%= history.datetime_select :to, id: :reservation_to %>
    </div>


      <%= history.submit %>
<% end %>

<div class="btn btn-success topcorner">
  <%= link_to 'Home', cars_url, :style =>'color:#FFFFFF;'%></div>